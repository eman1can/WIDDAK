<sequence values="BIEW">
  <one in="BB*/BBB/*B*" out="***/*I*/***"/>
  <all in="*I*/IBI" out="***/*I*"/>
  <all in="*B*/BIB/*B*" out="***/*W*/***"/>
  <one in="I" out="E" steps="2"/>
  <markov>
    <all in="EI" out="*E"/>
    <one in="EBI/EBI" out="**E/**E"/>
  </markov>
  <all in="E*W" out="**E"/>
  <prl>
    <rule in="I" out="B"/>
    <rule in="W" out="B"/>
  </prl>
</sequence>
