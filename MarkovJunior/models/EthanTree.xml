<sequence values="BWGNR" symmetry="(xy)" origin="True">
  <texturemap>
    <li>air</li>
    <li>wood_log</li>
    <li>leaves</li>
  </texturemap>
  <!-- Make the B cubes into R ones -->
  <one in="B" out="R"/>
  <!-- Turn all center Rs into B -->
  <one in="***/***/*** ***/*R*/*** ***/***/***" out="***/***/*** ***/*B*/*** ***/***/***"/>
  <!-- Create the bottom layer of N -->
  <one in="B B B B R" out="B N N N N"/>
  <!-- Remove the R border -->
  <one in="R" out="B"/>
  <!-- Expand trunk vertically -->
  <one in="* W *" out="W W W"/>
  <!-- Build leaves on each side -->
  <one in="BWB" out="GWG"/>
  <!-- Remove Bottom Spacing -->
  <one in="N" out="B"/>
  <!-- Randomize the leaves -->
  <one in="BGB" out="GGG" p="0.5" steps="20"/>
  <one in="GG" out="GB" p="0.4" steps="15"/>
  <one in="G G" out="G B" p="0.3" steps="10"/>
</sequence>